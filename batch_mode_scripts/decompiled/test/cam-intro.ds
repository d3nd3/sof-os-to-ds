nullx2.movetype = MOVETYPE_NOCLIP
camx3.movetype = MOVETYPE_NOCLIP
nullx3.movetype = MOVETYPE_NOCLIP
use entity camx
move entity camx by [-250.000 ,0.000 ,0.000] at 35 speed
